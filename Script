local VapeTab = Window:MakeTab({
	Name = "Vape",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local RapidTab = Window:MakeTab({
	Name = "Rapid",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

RapidTab:AddToggle({
	Name = "Rapid Fire",
	Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/god86521/base-battles/main/autofire"))()
		print(Value)
	end    
})

VapeTab:AddButton({
	Name = "Vape For Aim assist",
	Callback = function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
		print(Value)
	end    
})

